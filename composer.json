{
  "name": "richardhj/contao-simple-ajax",
  "description": "More control over your AJAX requests.",
  "keywords": [
    "contao",
    "ajax",
    "http",
    "network"
  ],
  "type": "contao-module",
  "homepage": "http://www.leo-unglaub.net",
  "license": "LGPL-3.0+",
  "authors": [
    {
      "name": "Leo Unglaub",
      "email": "leo@leo-unglaub.net",
      "homepage": "http://www.leo-unglaub.net",
      "role": "Developer"
    },
    {
      "name": "Richard Henkenjohann",
      "email": "richardhenkenjohann@googlemail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^5.3 || ^7.0",
    "contao/core": "~3.2",
    "contao-community-alliance/composer-plugin": "~2.4",
    "contao-community-alliance/event-dispatcher": "~1.3"
  },
  "replace": {
    "contao-legacy/simple_ajax": "*",
    "leounglaub/contao-simple-ajax": "*"
